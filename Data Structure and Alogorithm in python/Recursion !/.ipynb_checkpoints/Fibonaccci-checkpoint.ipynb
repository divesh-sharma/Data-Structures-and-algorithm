    "def fib(n):\n",
    "    if(n==1 or n==2):\n",
    "        return 1\n",
    "    fib_1 = fib(n-1)\n",
    "    fin_2 = fib(n-2)\n",
    "    return fib_1 + fin_2"
   ]
