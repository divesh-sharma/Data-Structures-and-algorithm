   "source": [
    "def replaceChar(s,a,b):\n",
    "    if(len(s)==0):\n",
    "        return s\n",
    "    smallOutput = replaceChar(s[1:],a,b)\n",
    "    if(s[0]==a):\n",
    "        return b + smallOutput\n",
    "    else:\n",
    "        return s[0] + smallOutput"
   ]
